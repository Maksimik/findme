    "eslint"
  ],
  "babel": {
    "env": {
      "development": {
        "presets": [
          "es2015",
          "react",
          "stage-0"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-to-generator",
          "transform-es2015-shorthand-properties",
          "transform-runtime",
          "transform-decorators-legacy"
        ],
        "ignore": [
          "client/bower_component",
          "client/out",
          "node_modules",
          ".ebextensions",
          "_db/**/*.sql",
          "build",
          "design",
          "ios",
          "legacy"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "react",
          "stage-0"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-to-generator",
          "transform-es2015-shorthand-properties",
          "transform-runtime",
          "transform-decorators-legacy"
        ],
        "ignore": [
          "client/bower_component",
          "client/out",
          "node_modules",
          ".ebextensions",
          "_db/**/*.sql",
          "build",
          "design",
          "ios",
          "legacy"
        ]
      }
    }
  },
